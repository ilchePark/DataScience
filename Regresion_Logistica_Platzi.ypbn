   "source": [
    "pca = PCA(n_components= 3)\n",
    "pca.fit(X_train)\n",
    "\n",
    "\n",
    "ipca = IncrementalPCA(n_components = 3, batch_size = 10)\n",
    "ipca.fit(X_train)\n",
