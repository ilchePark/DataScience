   "source": [
    "x = dataset[[\"gdp\",\"family\",\"lifexp\",\"freedom\",\"corruption\",\"generosity\",\"dystopia\"]]\n",
    "y = dataset[[\"score\"]]\n",
    "\n",
    "print(x.shape) ### FEATURES\n",
    "print(y.shape) ### TARGET\n",
    "\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 67,
   "id": "858b555b",
   "metadata": {},
   "outputs": [],
   "source": [
    "X_train, X_test, y_train, y_test = train_test_split(x,y, test_size= 0.25)"
   ]
  },
